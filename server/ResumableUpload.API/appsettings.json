{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UploadSettings": {
    "ChunkStoragePath": "uploads/chunks",
    "CompletedFilesPath": "uploads/files",
    "MaxRequestSize": 10485760,
    "AllowedExtensions": [
      ".jpg",
      ".jpeg",
      ".png",
      ".gif",
      ".pdf",
      ".docx",
      ".xlsx",
      ".pptx",
      ".zip",
      ".mp4",
      ".mov",
      ".mp3"
    ]
  }
}